\par {\listtext\pard\plain\ltrpar \rtlch\fcs1 \af3 \ltrch\fcs0 \f3\fs22 \loch\af3\dbch\af0\hich\f3 \'b7\tab}}\pard \ltrpar\ql \fi-427\li1440\ri0\widctlpar\wrapdefault\faauto\ls2\ilvl1\rin0\lin1440\itap0 {\rtlch\fcs1 \af0 \ltrch\fcs0 \fs22\insrsid2187702 
